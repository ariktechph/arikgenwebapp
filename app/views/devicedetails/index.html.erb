<p id="notice"><%= notice %></p>

<h3>Purchasing</h3>

<table class="table table-striped table-bordered table-hover">
  <thead class="thead-dark">
    <tr>
      <th>IMEI</th>
      <th>Brand</th>
      <th>Model</th>
      <th>Device type</th>
      <th>RAM</th>
      <th>Built in memory</th>
      <th>Battery</th>
      <th>Operating system</th>
      <th>Chipset</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @devicedetails.each do |devicedetail| %>
      <tr>
        <td contenteditable="true"><%= devicedetail.imei %></td>
        <td><%= devicedetail.brand %></td>
        <td><%= devicedetail.model %></td>
        <td><%= devicedetail.device_type %></td>
        <td><%= devicedetail.ram %></td>
        <td><%= devicedetail.built_in_mem %></td>
        <td><%= devicedetail.battery %></td>
        <td><%= devicedetail.op_sys %></td>
        <td><%= devicedetail.chipset %></td>
        <td><%= link_to 'Show', devicedetail %></td>
        <td><%= link_to 'Edit', edit_devicedetail_path(devicedetail) %></td>
        <td><%= link_to 'Destroy', devicedetail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Devicedetail', new_devicedetail_path %>
